
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="container">
    <div class="row  p-4">
        <div class="col-9">
            <h2 class="text-primary">Användarlista </h2>
        </div>
        <div class="col-3 text-right">
            <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register"><i class="fas fa-plus"></i> &nbsp; Skapa ny användare</a>
        </div>
    </div>

    <br />
    <div class="container">
        <div class="row  p-4">
            <div class="col-12">
                <div class="p-4 border rounded" style="width:100%">
                    <table id="tblData" class="table table-striped table-bordered">
                        <thead class="thead-dark">
                            <tr class="table-info">
                                <th scope="col">Namn</th>
                                <th scope="col">E-Post</th>
                                <th scope="col">Telefonnummer</th>
                                <th scope="col">Roll</th>
                                <th scope="col"></th>
                                
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{

    <script src="~/js/user.js"></script>

}
